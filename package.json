{
  "name": "@seognil-lab/webpack-starter",
  "version": "0.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:prod --",
    "serve": "npm run start --",
    "server": "npm run start --",
    "format": "npm run format:all --",
    "NOTICE": "# ---------------- lines above is alias",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "start": "webpack-dev-server --open",
    "format:all": "eslint --fix . && prettier --write '{,!(node_modules|dist)/**/}*.*'",
    "format:staged": "pretty-quick --staged && lint-staged",
    "analyze": "source-map-explorer './dist/**/*.js'"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:staged",
      "pre-push": "npm run format:staged"
    }
  },
  "publishConfig": {
    "registry": "https://prevent.push.to.registry/"
  },
